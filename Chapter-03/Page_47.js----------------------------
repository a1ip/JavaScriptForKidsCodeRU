var animals = [];
animals.push("Кот");
// 1
animals.push("Пес");
// 2
animals.push("Llama");
// 3
animals;
// ["Кот", "Пес", "Llama"]
animals.length;
// 3

animals;
// ["Кот", "Пес", "Llama"]
animals[0];
// "Кот"
animals.unshift("Мартышка");
// 4
animals;
// ["Мартышка", "Кот", "Пес", "Llama"]
animals.unshift("Белый медведь");
// 5
animals;
// ["Белый медведь", "Мартышка", "Кот", "Пес", "Llama"]
animals[0];
// "Белый медведь"
animals[2];
// "Кот"

animals;
// ["Белый медведь", "Мартышка", "Кот", "Пес", "Llama"]
var lastAnimal = animals.pop();
lastAnimal;
// "Llama"
animals;
// ["Белый медведь", "Мартышка", "Кот", "Пес"]
animals.pop();
// "Пес"
animals;
// ["Белый медведь", "Мартышка", "Кот"]
animals.unshift(lastAnimal);
// 4
animals;
["Llama", "Белый медведь", "Мартышка", "Кот"]

animals;
// ["Llama", "Белый медведь", "Мартышка", "Кот"]
var firstAnimal = animals.shift();
firstAnimal;
// "Llama"
animals;
// ["Белый медведь", "Мартышка", "Кот"]
